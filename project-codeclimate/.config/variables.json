{
  "codeClimateSpecVersion": "0.3.1",
  "docs": {
    "header_description_pre": "A slim Docker image and CodeClimate engine maintained by ",
    "header_title_pre": "<i></i>",
    "link": "https://megabyte.space/docs/docker/ci"
  },
  "gitlab_pipelines": [
    {
      "active": true,
      "cron": "0 11 * * 1",
      "description": "Weekly Build",
      "ref": "master",
      "variable": {
        "WEEKLY_BUILD": true
      }
    },
    {
      "active": true,
      "cron": "0 12 * * 1",
      "description": "Weekly Test",
      "ref": "master",
      "variable": {
        "WEEKLY_TEST": true
      }
    },
    {
      "active": true,
      "cron": "0 5 1 * *",
      "description": "Monthly Repository Update",
      "ref": "synchronize",
      "variable": {
        "REPOSITORY_UPDATE": true
      }
    }
  ],
  "repository": {
    "prefix": {
      "github": "codeclimate-"
    }
  },
  "subgroup": "codeclimate"
}
